{
	"name": "Utilitracker V2.0",
	"version": "2.0",
	"manifest_version": 2,
	"description": "Ground-up implementation of our extension",
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"http://*/*",
		"https://*/*",
		"webRequest",
		"webRequestBlocking",
		"tabs", 
		"storage",
		"notifications"
	],
	"web_accessible_resources": [
		"icon.png",
		"ask_offer.html",
		"countdown.html",
		"chrome-extension://*"
	]
}
