{
	"name": "Utilitracker V2.0",
	"version": "2.0",
	"manifest_version": 2,
	"description": "Ground-up implementation of our extension",
	"background": {
		"scripts": ["util.js", "background.js"]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs", 
		"storage"
	]
}